        <tr class="active-row">
            <td style="border: 1px solid black;border-bottom:0px;"><center><span t-field="do.price_subtotal"/></center></td>
            <td style="border: 1px solid black;border-bottom:0px;"><center><span t-field="do.price_unit"/></center></td>
            <td style="border: 1px solid black;border-bottom:0px;"><center><span t-field="do.quantity"/></center></td>
            <td style="border: 1px solid black;border-bottom:0px;"><center><span t-field="do.product_id.ar_name"/></center></td>
            <td style="border: 1px solid black;border-bottom:0px;"><center><t t-esc="i"/></center></td>
        </tr>
            <t t-set="i" t-value="i+1"/>
        </t>
        <tr class="active-row">
            <td style="border: 1px solid black;border-bottom:0px;border-top:0px;"><span t-field="o.amount_untaxed"/></td>
            <td colspan="2" style="border: 1px solid black;border-bottom:0px;border-right:0px;border-top:0px;">Subtotal</td>
